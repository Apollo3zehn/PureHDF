{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Benchmark: Inflate",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-benchmark",
            "program": "${workspaceFolder}/artifacts/bin/PureHDF.Benchmarks/Release/net7.0/PureHDF.Benchmarks.dll",
            "args": [
                "inflate"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark: Walk",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-benchmark",
            "program": "${workspaceFolder}/artifacts/bin/PureHDF.Benchmarks/Release/net7.0/PureHDF.Benchmarks.dll",
            "args": [
                "walk"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark: Shuffle",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-benchmark",
            "program": "${workspaceFolder}/artifacts/bin/PureHDF.Benchmarks/Release/net7.0/PureHDF.Benchmarks.dll",
            "args": [
                "shuffle"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark: Concurrency",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-concurrency-benchmark",
            "program": "${workspaceFolder}/artifacts/bin/PureHDF.ConcurrencyBenchmark/Release/net7.0/PureHDF.ConcurrencyBenchmark.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "externalTerminal"
        },
        {
            "name": "Debug: Source Generator",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickRemoteProcess}"
        }
    ]
}