{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "../src/PureHDF"
        }
      ],
      "dest": "api/netstandard2.0",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "../src/PureHDF"
        }
      ],
      "dest": "api/netstandard2.1",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.1"
      }
    },
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "../src/PureHDF"
        }
      ],
      "dest": "api/net5.0",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net5.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "src": "../src/PureHDF"
        }
      ],
      "dest": "api/net6.0",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net6.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.md"
        ]
      },
      {
        "files": "*.yml",
        "src": "api/netstandard2.0",
        "dest": "api/netstandard2.0",
        "version": ".NET Standard 2.0"
      },
      {
        "files": "*.yml",
        "src": "api/netstandard2.1",
        "dest": "api/netstandard2.1",
        "version": ".NET Standard 2.1"
      },
      {
        "files": "*.yml",
        "src": "api/net5.0",
        "dest": "api/net5.0",
        "version": ".NET 5"
      },
      {
        "files": "*.yml",
        "src": "api/net6.0",
        "dest": "api/net6.0",
        "version": ".NET 6"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "PureHDF",
      "_appFooter": "Copyright Â© 2023 Vincent Wilms",
      "_appFaviconPath": "images/icon.ico",
      "_appLogoPath": "images/logo.svg"
    },
    "fileMetadataFiles": [],
    "template":["default","templates/material"],
    "markdownEngineName": "markdig"
  }
}