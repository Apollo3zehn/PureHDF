{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net70"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "PureHDF",
      "_appFooter": "Copyright Â© 2023 Vincent Wilms",
      "_appFaviconPath": "images/icon.ico",
      "_appLogoPath": "images/logo.svg"
    },
    "fileMetadataFiles": [],
    "template":["default","templates/material"],
    "markdownEngineName": "markdig"
  }
}