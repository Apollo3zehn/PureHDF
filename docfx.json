{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "artifacts/_api",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net70"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "doc/*.md",
          "doc/toc.yml"
        ]
      },
      {
        "files": [
          "artifacts/_api/**.yml",
          "artifacts/_api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "doc/images/**"
        ]
      }
    ],
    "dest": "artifacts/docfx",
    "globalMetadata": {
      "_appTitle": "PureHDF",
      "_appFooter": "Copyright Â© 2023 Vincent Wilms",
      "_appFaviconPath": "doc/images/icon.ico",
      "_appLogoPath": "doc/images/logo.svg"
    },
    "fileMetadataFiles": [],
    "template":["default","templates/material"],
    "markdownEngineName": "markdig"
  }
}